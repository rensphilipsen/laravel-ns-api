{
    "name": "rensphilipsen/laravel-ns-api",
    "description": "A Laravel wrapper for the Nederlandse Spoorwegen (NS) API",
    "license": "MIT",
    "authors": [
        {
            "name": "Rens Philipsen",
            "email": "rens.philipsen@screenlink.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^7.0",
        "guzzlehttp/guzzle": "^6.0",
        "ext-json": "*",
        "laravel-frontend-presets/tall": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "RensPhilipsen\\NSApi\\" : "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RensPhilipsen\\NSApi\\NSApiServiceProvider"
            ],
            "aliases": {
                "NSApi": "RensPhilipsen\\NSApi\\Facade"
            }
        }
    }
}
